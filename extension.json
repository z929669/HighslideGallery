{
	"name": "HighslideGallery",
	"version": "2.1.1",
	"author": ["Brian McCloskey", "David Van Winkle", "Step Modifications, Inc."],
	"license-name": "GPL-2.0-only",
	"url": "https://www.mediawiki.org/wiki/Extension:HighslideGallery",
	"descriptionmsg": "hsg-desc",
	"type": "parserhook",
	"requires": {
		"MediaWiki": ">= 1.39.0"
	},
	"MessagesDirs": {
		"HighslideGallery": ["i18n"]
	},
	"ExtensionMessagesFiles": {
		"HighslideGalleryMagic": "i18n/HighslideGallery.i18n.magic.php"
	},
	"AutoloadClasses": {
		"HighslideGallery": "HighslideGallery.body.php"
	},
	"ResourceFileModulePaths": {
		"localBasePath": "modules",
		"remoteExtPath": "HighslideGallery/modules"
	},
	"ResourceModules": {
		"ext.highslideGallery": {
			"scripts": ["highslide-full.js", "highslide.cfg.js"],
			"styles": ["highslide.css", "highslide.override.css"],
			"dependencies": ["jquery", "mediawiki.util"],
			"messages": [
				"hsg-hover-instructions",
				"hsg-loading-text",
				"hsg-loading-title",
				"hsg-focus-title",
				"hsg-full-expand-title",
				"hsg-previous-title",
				"hsg-next-title",
				"hsg-move-title",
				"hsg-close-title",
				"hsg-play-title",
				"hsg-pause-title",
				"hsg-number"
			],
			"position": "top"
		}
	},
	"config_prefix": "wg",
	"config": {
		"HSGControlsPreset": {
			"value": "classic",
			"description": "HighslideGallery control + thumbstrip layout preset (e.g. classic, stacked-top).",
			"public": true
		}
	},
	"Hooks": {
		"BeforePageDisplay": "HighslideGallery::AddResources",
		"ImageBeforeProduceHTML": "HighslideGallery::MakeImageLink",
		"ParserFirstCallInit": "HighslideGallery::AddHooks",
		"ResourceLoaderGetConfigVars": "HighslideGallery::onResourceLoaderGetConfigVars"
	},
	"manifest_version": 2
}
